{
  "name": "px-qrcode",
  "description": "QRCodes with server side support",
  "version": "1.5.1",
  "main": "./lib/index.js",
  "browser": {
    "./lib/index.js": "./lib/browser.js",
    "fs": false
  },
  "files": [
    "bin",
    "build",
    "lib",
    "helper"
  ],
  "scripts": {
    "pretty": "prettier --write \"{,!(.next|database)/**/}*.{js,jsx,json,ts,tsx,css}\""
  },
  "dependencies": {
    "dijkstrajs": "^1.0.1",
    "encode-utf8": "^1.0.3",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "prettier": "^2.1.2"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "printWidth": 80,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
